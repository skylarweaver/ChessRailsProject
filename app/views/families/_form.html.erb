<%= simple_form_for(@family) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :family_name %>
    <%= f.input :parent_first_name %>
    <%= f.input :email %>
    <%= f.input :phone %>
    <%= f.input :active %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
