<%= simple_form_for(@registration) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :camp_id %>
    <%= f.input :student_id %>
    <%= f.input :payment_status %>
    <%= f.input :points_earned %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
